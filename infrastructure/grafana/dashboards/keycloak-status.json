{
  "uid": "keycloak-status",
  "title": "Keycloak Status",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "tags": ["payment-system", "keycloak"],
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Keycloak UP",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=\"keycloak\"}",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Keycloak scrape duration (s)",
      "gridPos": { "x": 8, "y": 0, "w": 16, "h": 6 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "scrape_duration_seconds{job=\"keycloak\"}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Keycloak samples scraped",
      "gridPos": { "x": 0, "y": 6, "w": 24, "h": 7 },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" },
      "targets": [
        {
          "refId": "A",
          "expr": "scrape_samples_scraped{job=\"keycloak\"}"
        }
      ]
    }
  ],
  "time": { "from": "now-15m", "to": "now" }
}